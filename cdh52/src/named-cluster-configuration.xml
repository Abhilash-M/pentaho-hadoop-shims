<?xml version="1.0" encoding="UTF-8"?>
<namedconfig>
	<name>cdh52</name>
	<type>hadoop-cluster</type>
	<groups>
		<group>
			<name>NameNode</name>
			<properties>
				<property>
					<propertyName>hostname</propertyName>
					<propertyValue>localhost</propertyValue>
					<displayName>Hostname</displayName>
					<type>java.lang.String</type>
					<defaultValues>
					</defaultValues>
				</property>
				<property>
					<propertyName>port</propertyName>
					<propertyValue>50070</propertyValue>
					<displayName>Port</displayName>
					<type>java.lang.Integer</type>
					<defaultValues>
					</defaultValues>
				</property>
			</properties>
		</group>	
		<group>
			<name>JobTracker</name>
			<properties>
				<property>
					<propertyName>hostname</propertyName>
					<propertyValue>localhost</propertyValue>
					<displayName>Hostname</displayName>
					<type>java.lang.String</type>
					<defaultValues>
					</defaultValues>
				</property>
				<property>
					<propertyName>port</propertyName>
					<propertyValue>50030</propertyValue>
					<displayName>Port</displayName>
					<type>java.lang.Integer</type>
					<defaultValues>
					</defaultValues>
				</property>
			</properties>
		</group>
		<group>
			<name>HDFS</name>
			<properties>
				<property>
					<propertyName>hostname</propertyName>
					<propertyValue>localhost</propertyValue>
					<displayName>Hostname</displayName>
					<type>java.lang.String</type>
					<uiType>textbox</uiType>
					<defaultValues>
					</defaultValues>
				</property>
				<property>
					<propertyName>port</propertyName>
					<propertyValue>10000</propertyValue>
					<displayName>Port</displayName>
					<type>java.lang.Integer</type>
					<uiType>textbox</uiType>
					<defaultValues>
					</defaultValues>
				</property>
				<property>
					<propertyName>username</propertyName>
					<propertyValue>hdfs_user</propertyValue>
					<displayName>Username</displayName>
					<type>java.lang.String</type>
					<uiType>textbox</uiType>
					<defaultValues>
					</defaultValues>
				</property>
				<property>
					<propertyName>password</propertyName>
					<propertyValue></propertyValue>
					<displayName>Password</displayName>
					<type>password</type>
					<uiType>textbox</uiType>
					<defaultValues>
					</defaultValues>
				</property>								
			</properties>
		</group>
		<group>
			<name>Oozie</name>
			<properties>
				<property>
					<propertyName>url</propertyName>
					<propertyValue>http://localhost:11000/oozie</propertyValue>
					<displayName>URL</displayName>
					<type>java.lang.String</type>
					<defaultValues>
					</defaultValues>
				</property>
			</properties>
		</group>			
	</groups>
</namedconfig>